    "gh-pages": "^6.3.0",
    "jiti": "^2.4.2",
    "npm-run-all2": "^7.0.2",
    "oxlint": "^0.15.10",
    "prettier": "^3.5.1",
    "sass": "^1.87.0",
    "typescript": "~5.7.3",
    "unocss": "^66.1.0",
    "unplugin-auto-import": "^19.1.0",
    "unplugin-vue-components": "^28.4.0",
    "vite": "^6.1.0",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vue-tsc": "^2.2.2"
