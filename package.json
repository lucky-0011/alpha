    "element-plus": "^2.10.2",
    "mitt": "^3.0.1",
    "nprogress": "^0.2.0",
    "pinia": "^3.0.1",
    "pinia-plugin-persistedstate": "^4.2.0",
    "vue": "^3.5.13",
    "vue-i18n": "^11.1.6",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@iconify/vue": "^5.0.0",
    "@intlify/unplugin-vue-i18n": "^6.0.8",
